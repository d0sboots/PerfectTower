key.t()

:import rnglib
:budget_cap max

:global double predicted_rng
:global double actual_rng
:global int correct
:global int wrong

:local int i
:local string which

correct = 0
wrong = 0

{rng_init(guess)}

guess:
{rng_next}
predicted_rng = {rng_getuniform}
actual_rng = rnd(0., 1.)
which = if(predicted_rng == actual_rng, "correct", "wrong")
gis(which, 1 + gig(which))
i += 1
gotoif(guess, i < 10000)
