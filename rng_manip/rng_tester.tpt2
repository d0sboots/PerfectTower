key.t()

:import rnglib
:budget_cap max

:global int predicted_rng
:global int actual_rng
:global int correct
:global int wrong

:local int i
:local string which

correct = 0
wrong = 0

{rng_init1}
{rng_init2(guess)}

guess:
{rng_next}
predicted_rng = {rng_getrange(0, 999999)}
